    field(DESC, "Error")
    field(CALC, "(B>0)?0:(A=24)?7:A")
    field(INPA, "$(device):ERR MS")
    field(INPB, "$(device):CALWARN PP")
    field(HIHI, "1")
    field(LOLO, "-1")
    field(HHSV, "MAJOR")
    field(LLSV, "MAJOR")
}
