################################################################################
# RESET
#
record(bo, "$(device):RESET")
{
    field(DESC, "Reset")
    field(DTYP, "stream")
    field(OUT,  "@cp2800.proto reset($(device)) $(port)")
    field(ZNAM, "Reset")
    field(SDIS, "$(device):DISABLE")
    field(FLNK, "$(device):ACKPF")
}
################################################################################
# ACKPF - Clear RSPF bit
#
record(bo, "$(device):ACKPF")
{
    field(DESC, "Clear RSPF bit")
    field(DTYP, "stream")
    field(OUT,  "@cp2800.proto ack_pf($(device)) $(port)")
# automatically reset - to maintain comms
#    field(SCAN, "10 second")
    field(ZNAM, "Clear")
    field(SDIS, "$(device):DISABLE")
    field(FLNK, "$(device):CLRRSP")
}
