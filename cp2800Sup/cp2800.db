# cp2800.template
# Template database for the Cryomech CP2800 series compressor
# Pete Owens 8/1/2012
#
# NOTE: Stream device must include SMPD format converter "%!"
#
# substitutions:
# device  - device name
# port    - asyn port
#
record(stringout, "$(device):TEST") 
{
    field(DESC, "TEST")
    field(DTYP, "stream")
    field(OUT,  "@cp2800.proto test($(device)) $(port)")
}
################################################################################
# Disable Control
#
record(bi, "$(device):DISABLE")
{
    field(DESC, "Disable Control")
    field(ZNAM, "Enable")
    field(ONAM, "Disable")
    field(OSV,  "MAJOR")
    field(VAL,  "0")
    field(PINI, "YES")
}
################################################################################
# CMDRSP - Command Response byte 
# Parse on mismatch
#
record(mbbiDirect, "$(device):CMDRSP") 
{
    field(DESC, "Command Response")
}
record(calcout,"$(device):CLRRSP")
{
    field(DESC, "Clear Response")
    field(OUT,  "$(device):CMDRSP PP")
    field(CALC, "A")
    field(INPA, "1")
    field(PINI, "YES")
}
record(calc, "$(device):CMD") 
{
    field(DESC, "Command")
    field(CALC, "A>>4")
    field(INPA, "$(device):CMDRSP MS")
    field(SCAN, "1 second")
    field(PHAS, "2")
    field(FLNK, "$(device):RSPF")
}
record(bi, "$(device):RSPF")
{
    field(DESC, "Reset since ack power fail")
    field(INP,  "$(device):CMDRSP.B3 MS")
    field(ZNAM, "No")
    field(ONAM, "Yes")
    field(OSV,  "MAJOR")
    field(FLNK, "$(device):RSP")
}
record(mbbi, "$(device):RSP") 
{
    field(DESC, "Response")
    field(DTYP, "Raw Soft Channel")
    field(INP,  "$(device):CMDRSP MS")
    field(NOBT, "3")
    field(SHFT, "0")
    field(UNSV, "INVALID")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(TWVL, "2")
    field(THVL, "3")
    field(FRVL, "4")
    field(FVVL, "5")
    field(SXVL, "6")
    field(ZRST, "Illegal RSP")
    field(ZRSV, "MAJOR")
    field(ONST, "OK")
    field(TWST, "Invallid Cmd")
    field(TWSV, "MAJOR")
    field(THST, "Syntax Error")
    field(THSV, "MAJOR")
    field(FRST, "Range Error")
    field(FRSV, "MINOR")
    field(FVST, "Inhibited")
    field(FVSV, "MINOR")
    field(SXST, "Obsolete Cmd")
    field(SXSV, "MINOR")
}
